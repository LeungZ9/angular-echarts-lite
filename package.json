{
  "name": "angular-echarts-lite",
  "version": "1.0.9",
  "description": "angular directive for baidu echarts",
  "scripts": {
    "build": "mkdirp dist && webpack --progress --colors",
    "build:min": "mkdirp dist && webpack --progress --colors",
    "dist": "npm run clean && npm run build && npm run build:min",
    "clean": "del-cli dist/**",
    "dev": "npm run clean && webpack-dev-server --progress --open"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/LeungZ9/angular-echarts-lite.git"
  },
  "keywords": [
    "angular",
    "baidu",
    "echarts"
  ],
  "author": "LeungZ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LeungZ9/angular-echarts-lite/issues"
  },
  "homepage": "https://github.com/LeungZ9/angular-echarts-lite",
  "dependencies": {
    "angular": "^1.2.0",
    "echarts": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.23.0",
    "eslint": "^4.2.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-angular": "^3.1.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1",
    "del-cli": "^1.0.0",
    "mkdirp": "^0.5.1"
  }
}